   "source": [
    "hev.set_particle(cpart)\n",
    "hev.get_xdepth()\n",
    "\n",
    "etot = 0\n",
    "for cp in hev.get_products():\n",
    "    etot += cp.energy\n",
    "    \n",
    "print(etot)    "
